[uwsgi]
plugins      = python34
chdir        = ${ROOT}
home         = ${ROOT}/usr
pythonpath   = ${ROOT}
module       = boot.wsgi
daemonize    = ${ROOT}/var/log/uwsgi.log
pidfile      = ${PIDFILE}
socket       = ${SOCKET}
chown-socket = ${USER}:${GROUP}
chmod-socket = 664
vacuum       = True
env          = DJANGO_SETTINGS_MODULE=boot.active.settings

master       = true
processes    = 2
