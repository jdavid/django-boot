[uwsgi]
plugins      = python34
chdir        = ${ROOT}
home         = ${ROOT}/usr
pythonpath   = ${ROOT}
module       = boot.wsgi
pidfile      = ${PIDFILE}
vacuum       = True
env          = DJANGO_SETTINGS_MODULE=boot.active.settings

master       = true
master-fifo  = ${FIFO}
processes    = 2

http         = 127.0.0.1:8000
