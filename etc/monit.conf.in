check process ${NAME} with pidfile ${PIDFILE}
   start program = "/usr/bin/uwsgi ${ROOT}/etc/uwsgi.ini"
                   as uid ${USER} and gid ${GROUP}
   stop  program = "/usr/bin/uwsgi --stop ${PIDFILE}
                   as uid ${USER} and gid ${GROUP}
   if failed unixsocket ${SOCKET} type TCP then restart
   if 5 restarts within 5 cycles then timeout
