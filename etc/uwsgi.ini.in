[uwsgi]
plugins      = python27
chdir        = ${ROOT}
home         = ${ROOT}/usr
pythonpath   = ${ROOT}
module       = project.wsgi
daemonize    = ${ROOT}/var/uwsgi.log
pidfile      = ${PIDFILE}
socket       = ${SOCKET}
chown-socket = ${USER}:${GROUP}
chmod-socket = 664
vacuum       = True
env          = DJANGO_SETTINGS_MODULE=project.settings.${TARGET}

#master       = true
#processes    = 1
